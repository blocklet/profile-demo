    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "morgan": "^1.10.0",
    "notistack": "^3.0.1",
    "p-all": "^5.0.0",
    "qrcode.react": "^3.1.0",
    "ua-parser-js": "^1.0.37"
  },
  "devDependencies": {
