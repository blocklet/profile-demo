    "@vitejs/plugin-react": "^4.2.1",
    "dayjs": "^1.11.10",
    "dotenv-flow": "^4.1.0",
    "flat": "^6.0.1",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.2.8",
    "vite-plugin-blocklet": "^0.7.9"
  },
