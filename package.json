{
  "name": "profile-demo",
  "version": "1.14.2",
  "main": "index.js",
  "author": "ArcBlock",
  "license": "MIT",
  "scripts": {
    "dev": "blocklet dev",
    "lint": "eslint src api",
    "clean": "rm -rf build",
    "start": "vite-node -c vite.config.server.js -w api/dev.js",
    "bundle": "npm run clean && npm run bundle:client && npm run bundle:server",
    "bundle:client": "vite build",
    "bundle:server": "blocklet bundle --compact --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "coverage": "npm run test -- --coverage",
    "update:deps": "npx taze -w && npm run dedupe",
    "dedupe": "npx yarn-deduplicate yarn.lock && yarn",
    "bump-version": "make bump-version"
  },
  "dependencies": {
    "@blocklet/sdk": "^1.16.28",
    "@ocap/mcrypto": "^1.18.126",
    "@ocap/util": "^1.18.126",
    "@ocap/wallet": "^1.18.126",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@arcblock/did-connect": "^2.10.5",
    "@arcblock/ux": "^2.10.5",
    "@blocklet/js-sdk": "^1.16.28",
    "@blocklet/ui-react": "^2.10.5",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.16.0",
    "@mui/material": "^5.16.0",
    "@vitejs/plugin-react": "^4.3.1",
    "dayjs": "^1.11.11",
    "dotenv-flow": "^4.1.0",
    "flat": "^6.0.1",
    "nodemon": "^3.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.24.1",
    "vite": "^5.3.3",
    "vite-node": "^2.0.2",
    "vite-plugin-blocklet": "^0.8.7",
    "vite-plugin-require": "^1.2.14"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
