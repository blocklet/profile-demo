{
  "name": "profile-demo",
  "version": "1.10.0",
  "main": "index.js",
  "author": "ArcBlock",
  "license": "MIT",
  "scripts": {
    "dev": "blocklet dev",
    "lint": "eslint src api",
    "clean": "rm -rf build",
    "start": "NODE_ENV=development nodemon api/dev.js -w api",
    "bundle": "npm run clean && npm run bundle:client && npm run bundle:server",
    "bundle:client": "vite build",
    "bundle:server": "blocklet bundle --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "coverage": "npm run test -- --coverage",
    "update:deps": "npx taze -w && npm run dedupe",
    "dedupe": "npx yarn-deduplicate yarn.lock && yarn",
    "bump-version": "make bump-version"
  },
  "dependencies": {
    "@blocklet/sdk": "^1.16.23",
    "@ocap/mcrypto": "^1.18.110",
    "@ocap/util": "^1.18.110",
    "@ocap/wallet": "^1.18.110",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@arcblock/did": "^1.18.110",
    "@arcblock/did-connect": "^2.9.38",
    "@arcblock/ux": "^2.9.38",
    "@blocklet/ui-react": "^2.9.38",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@iconify-icons/logos": "^1.2.36",
    "@mui/material": "^5.15.11",
    "@vitejs/plugin-react": "^4.2.1",
    "dayjs": "^1.11.10",
    "dotenv-flow": "^4.1.0",
    "flat": "^6.0.1",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "vite": "^5.1.4",
    "vite-plugin-blocklet": "^0.7.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
